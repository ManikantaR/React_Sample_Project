{
  "name": "SP-3",
  "version": "1.0.0",
  "description": "Seed Project 3",
    "nyc": {
    "exclude": [
      "node_modules",
      "bin",
      "coverage",
      "src/Home/*",
      "src/Styles/*",
      "tools/*",
      "src/Common/*"
    ]
  },
  "scripts": {
    "start": "npm-run-all --parallel test:watch open:src lint",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "gulp lint",
    "lint:build": "gulp lint:build",
    "build": "webpack -p --progress --config webpack.config.js",
    "css": "gulp css",
    "lint:watch": "npm run lint -- --watch",
    "test": "nyc --reporter=html --reporter=text mocha --reporter spec tools/testSetup.js \"test/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
